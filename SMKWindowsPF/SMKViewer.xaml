<Window x:Class="SMKWindowsPF.SMKViewer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SMKWindowsPF"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <GroupBox Header="Springall+ MK   P R O BIM TOOLS" Margin="5 0 5 5">
            <GroupBox.HeaderTemplate>
                <DataTemplate>
                    <TextBlock Text="{Binding}" FontSize="15" Foreground="DeepSkyBlue"/>
                </DataTemplate>
            </GroupBox.HeaderTemplate>
            <StackPanel>
                <TreeView Name="VIEWLIST" Height="280" Margin="5 5 5 5"/>
            </StackPanel>
        </GroupBox>
    </Grid>
</Window>
